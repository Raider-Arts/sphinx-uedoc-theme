{% if theme_color_scheme %}
{ 
	"default": {{ theme_color_scheme }}
}
{%- else %}
{
	"default": {
		"background": "#fff", 
		"primary": "hsl(0, 0%, 16%)", 
		"secondary": "#0aaff0",
		"text-primary": "hsl(0, 0%, 100%)",
		"text-secondary": "hsl(0, 0%, 100%)",
		"text-background": "hsl(0, 0%, 0%)"
	}
}
{% endif %}