.sidebar-item-expanded span.button{
  background: url(./images/button-open.png) no-repeat !important;
}

.sidebar-item-collapsed span.button{
  background: url(./images/button-closed.png) no-repeat !important;
}

div#sidebarwidgets > :not(#uedoc-globaltoc) {
  margin-left: 20px;
}

div#sidebarwidgets > h3 {
