  margin: 0;
  padding: 3px 8px;
  position: fixed;
  left: 300px;
  top: 152px;
  z-index: 1001;
  background-color: hsl(var(--clr-background-hs), 85%);
  color: var(--clr-text-background);
  cursor: pointer;
  border: 1px solid hsl(var(--clr-primary-hs), 63%);
  font-size: 2.5em;
}

/*#endregion Navigation */

/*#region Filter Pages */

@media only screen and (max-width: 1280px) {
  div#searchbox{
    padding-right: 42px;
  }
}

form.search {
  padding: 1px 0 1px 1px;
}

div.sphinxsidebar form {
  margin-top: 0 !important;
}

#searchbox input {
  position: relative;
  height: 32px;
  width: calc(100% - 32px) !important;
  padding: 4px 16px !important;
  border-width: 0 0 1px 0;
  border-color: #767676;
  box-sizing: initial !important;
  font-family: Arial;
  font-size: 0.85em;
}

/*#endregion Filter Pages  */

/*#endregion Sidebar */

/*#region localtoc */

div#uedoc-localtoc .current-reading > a{
  background-color: #d8d8d8;
  font-weight: bold;
}

div#localtoc h3 {
  position: relative;
  line-height: 1.3em;
  font-weight: bold;
  border-radius: 0 0 0 0;
  font-size: 1em;
  color: #000;
  letter-spacing: 1px;
  font-family: Brutal_Regular,Verdana,Arial,sans serif;
  text-transform: uppercase;
  height: 14px;
  padding: 0 20px;
}

div#uedoc-localtoc ul {
  list-style: none;
  padding: 0;
  margin: .325em 14px 0 30px;
}

div#uedoc-localtoc li ul {
  margin: .325em 0 0 0;
  padding: 0;
}

div#uedoc-localtoc li {
  line-height: 2.5em;
}

div#uedoc-localtoc li a {
  color: #000;
  opacity: 1;
  display: block;
  padding: 8px 8px;
  text-decoration: none;
  position: relative;
}

div#uedoc-localtoc li li a {
  padding-left: 20px;
}

div#uedoc-localtoc li li li a {
  padding-left: 40px;
}

div#uedoc-localtoc li li li li a {
  padding-left: 60px;
}

@media only screen and (max-width: 1280px) {
  div#localtoc {
    display: none;
  }
}

div#localtoc {
  float: right;
  position: absolute;
  top: 0;
  right: 0;
  height: 100%;
}

div.localtocwrapper {
  position: sticky;
  top: 20px;
  width: 312px;
  border: 1px solid #e0e0e0;
  font-family: Brutal_Regular,Verdana,Arial,sans serif;
  display: block;
  margin: 20px 36px 20px 0;
  overflow: hidden auto;
  text-overflow: ellipsis;
  background-color: var(--clr-background);
}

/*#endregion localtoc */

/*#region Related */

div.related {
  height: 83px;
  padding: 0;
  border-bottom: 1px solid #e0e0e0;
  overflow: hidden;
  display: flex;
  align-items: center;
}

div.related ul {
  padding: 28px;
  font-family: Brutal_Regular,Verdana,Arial,sans serif;
  font-size: 1.3em;
  float: left;
  /* width: calc(100% - 630px); */
  display: block;
}

@media only screen and (max-width: 1280px) {
  div.related ul {
    display: none;
  }
}

div.related a {
  text-decoration: none;
  opacity: 1;
  color: #000;
}

span.separator {
  margin: 0 6px;
  opacity: .3;
}

ul span#page {
  color: #000;
  opacity: .75;
  padding-right: 42px;
}

div#search_container {
  margin: 20px;
  margin-right: 8em;
  padding: 0;
  overflow: hidden;
  float: right;
  z-index: 100;
  top: 0;
  position: relative;
  height: 46px;
  width: 465px;
  margin-left: auto;
}

@media only screen and (max-width: 1280px){
  div#search_container{
    margin-left: auto;
    margin-right: auto;
    width: unset;
  }

  div#search_container .searchbox {
    width: unset;
  }
}

#search_container .searchbutton {
  width: 17px;
  height: 17px;
  padding: 10px;
  position: absolute;
  right: 5px;
  top: 3px;
  opacity: .25;
}

#search_container .searchbox {
  color: #000;
  opacity: .75;
  margin: 0;
  padding: 13px 42px 13px 17px;
  width: 404px;
  font-family: Brutal_Regular,Verdana,Arial,sans serif;
  font-size: 14px;
  border: 1px solid #e0e0e0;
  background: #fff;
}
/*#endregion Related */

/*#region Footer */

div.footer {
  font-size: 16px;
  font-family: OpenSans,sans-serif;
  -webkit-text-size-adjust: 100%;
  -moz-text-size-adjust: 100%;
  -ms-text-size-adjust: 100%;
  text-size-adjust: 100%;
  position: relative;
  padding: 1.25em 4.5em 2.5em;
  z-index: 10;

  border-color: #333!important;
  background-color: #2a2a2a;
  border-top: 1px solid #ccc;
}

div.footer, div.footer a{
  color: #e7e7e7;
}

div.social-links {
  margin-top: 1em;
  display: flex;
  flex-direction: row;
}

div.social-links > * {
  padding: 0 .3em;
  margin: 0 0 .5em;
}

div.social-links a{
  font-size: 1.75em;
}

div.footer a,
div.footer button,
div.footer button span{
  -webkit-transition: color .3s ease-in-out;
  -o-transition: color .3s ease-in-out;
  transition: color .3s ease-in-out;
}

div.footer button{
  -webkit-transition: border-color .3s ease-in-out;
  -o-transition: border-color .3s ease-in-out;
  transition: border-color .3s ease-in-out;
}

div.footer a:hover, 
div.footer button:hover,
div.footer button span:hover{
  color: #0aaff1
}

div.footer button:hover{
  border-color: #0aaff1
}

div.footer {
  color: #ccc;
}

div.footer ul.footer_links {
  width: fit-content;
  list-style: none;
  margin: 0;
  margin-top: 1em;
  margin-bottom: 0.75em;
  padding-left: 0;
  column-count: {{((theme_footerlinks | length)/(theme_linkpercolumn | int)) | int}};
}

ul.footer_links li {
  line-height: 1.5em;
  margin-right: 1.5em;
}

ul.footer_links a {
  text-decoration: none;
  font-size: .86em;
  font-family: OpenSans,sans-serif;
}

span.footer_copyright {
  font-size: .71em;
  font-family: OpenSans,sans-serif;
  font-weight: 400;
  line-height: 2em;
  padding-left: 1em;
}

button.sendto_top {
  color: #ccc;
  cursor: pointer;
  float: right;
  width: 2.1em;
  height: 2.1em;
  border: 2px solid #fff;
  padding: .25em;
  border-radius: 2px;
  margin: 1.25em 1em 0 0;
  font-size: 1.2em;
  -webkit-transition: border .3s ease-in-out;
  -o-transition: border .3s ease-in-out;
  transition: border .3s ease-in-out;
  background-color: transparent;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

/*#endregion Footer */

/*#region Search_page */

div.search_page h1 {
  font-size: 29px;
  font-weight: 600;
  line-height: 1.1;
  font-family: Brutal_Regular, "Open Sans", arial;
  margin: .67em 0;
  margin-top: 22px;
  margin-bottom: 11px;
}

div.search_page form {
  background-color: rgb(10, 175, 241);
  height: 45px;
  padding-left: 2px;
  position: relative;
  border-radius: 5px;
  overflow: hidden;
  margin: 0px;
}

div.search_page form input[type=text] {
  display: block;
  width: calc(100% - 108px);
  height: 41px;
  border-width: initial;
  border-style: none;
  border-color: initial;
  border-image: initial;
  padding: 0px 0px 0px 1em;
  margin: 2px 0px;
  outline: none;
}

div.search_page form input[type=submit] {
  position: absolute;
  height: 100%;
  width: 100px;
  right: 0px;
  top: 0px;
  font-weight: 500;
  text-align: center;
  color: white;
  font-size: 1.1em;
  padding: 0px;
  border-width: 0px;
  border-style: initial;
  border-color: initial;
  border-image: initial;
  transition: background 0.2s ease-in-out 0s;
  outline: none;
  background: transparent;
  font-family: OpenSans,sans-serif;
  font-size: 1.7em;
  cursor: pointer;
}

div.search_page div.body {
  width: unset;
  padding: 1.25em 0px;
}

div.search_page {
  display: flex;
  flex-direction: column;
  align-items: center;
}

div.search_page div.body {
  max-width: 1200px;
  padding-right: 15px;
  padding-left: 15px;
}

div.search_page li::before {
  font-family: "Font Awesome 5 Free";
  font-weight: 900; 
  content: "\f07c";
  position: absolute;
  left: 0;
}

div.search_page li {
  position: relative;
  background-image: none;
}

div.search_page li a:hover{
  color: #0aaff1
}

@media only screen and (min-width: 768px) {
  div.search_page div.body {
      width: 750px;
  }
}

@media only screen and (min-width: 992px) {
  div.search_page div.body {
      width: 970px;
  }
}

@media only screen and (min-width: 1200px) {
  div.search_page div.body {
      width: 1170px;
  }
}

/*#endregion Search_page */

