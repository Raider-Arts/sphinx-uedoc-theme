  font-size: 1em;
  margin-left: 0;
  margin-right: 0;
}

div#sidebar a {
  display: block;
  /* position: relative; */
  padding: 10px 5px 10px 5px;
  text-decoration: none;
  opacity: 1;
  margin-left: 10px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-family: Brutal_Regular;
