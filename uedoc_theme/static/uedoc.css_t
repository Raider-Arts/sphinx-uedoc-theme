}

div#sidebar::-webkit-scrollbar {
  width: 11px;
  right: 3px;
  z-index: 1020;
}

div#sidebarwidgets{
  overflow: hidden auto;
  margin-right: 2px;
}

div#sidebar ul {
  margin: 0;
  padding: 0;
  padding-left: 0;
  list-style-type: none;
}

div#sidebar li {
  font-size: 14px !important;
  background-color: transparent;
  position: relative;
  line-height: 1.75em;
}

div#sidebar li a {
  margin-left: 45px;
}

div#sidebar li li a {
  margin-left: 65px;
}

div#sidebar li li li a {
  margin-left: 85px;
}

div#sidebar li li li li a {
  margin-left: 105px;
}

div#sidebar li span.button {
  left: 25px;
}

div#sidebar li li span.button {
  left: 50px;
}

div#sidebar li li li span.button {
  left: 70px;
}

div#sidebar li li li li span.button {
  left: 90px;
}

div#sidebar li span.button {
  content: "";
