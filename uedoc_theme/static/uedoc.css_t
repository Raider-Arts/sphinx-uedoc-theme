  }
}

.uedoc-header #nav-menu {
  width: 100%;
  height: 100%;
}

.uedoc-header,
.uedoc-header #nav-menu .nav-menu-wrapper{
  width: 100%;
  clear: both;
  height: 54px;
  background-color: var(--clr-primary);
  display: flex;
  align-items: center;
}

.uedoc-header > * {
  margin-bottom: 5px;
}

@media only screen and (max-width: 1280px) {
  div.uedoc-title {
    font-size: 1.3em;
  }
}

@media only screen and (max-width: 767px) {
  div.uedoc-title {
    font-size: 1.1em;
    white-space: unset;
  }
}

@media only screen and (max-width: 450px) {
  div.uedoc-title {
    font-size: 1em;
  }
}

.uedoc-title {
  color: var(--clr-text-primary);
  font-family: Verdana;
  font-weight: 700;
  font-size: 1.5em;
  margin: 0 40px 0 20px;
  white-space: nowrap;
}

.uedoc-title a {
  text-decoration: none;
  color: unset;
}

.uedoc-logo {
  width: 3.7em;
  height: 3.7em;
  margin-left: .5em;
  background: url('{{theme_logo}}') 50% no-repeat;
  background-size: 50px auto;
}

.uedoc-quicklinks {
  list-style-type: none;
  height: 100%;
  padding: 0;
  margin: 0;
}

.uedoc-quicklinks li {
  position: relative;
  height: 100%;
}

.uedoc-quicklinks-wrapper {
  height: 100%;
}

.uedoc-quicklinks li::before {
  background: var(--clr-secondary);
  content: "";
  display: block;
  position: absolute;
  bottom: 0px;
  left: 0;
  width: 100%;
  height: 0;
  -webkit-transition: height .2s ease-in-out;
  -o-transition: height .2s ease-in-out;
  transition: height .2s ease-in-out;
}

.uedoc-quicklinks li:hover::before {
  height: 5px;
}

.uedoc-quicklinks p {
  color: hsl(var(--clr-text-primary-hs), 80%);
  margin: 0;
  font-size: .85em;
}


.uedoc-quicklinks a {
  display: block;
  font-family: OpenSans,sans-serif,arial;
  text-transform: uppercase;
  letter-spacing: .075em;
  padding: 0 1em;
  height: 100%;
  text-decoration: none;
  display: flex;
  align-items: center;
}

.uedoc-quicklinks a:visited {
  text-decoration: none;
}

@media only screen and (max-width: 767px) {
  .uedoc-header div.hamburger-nav {
    display: flex;
  }
}

.uedoc-header .hamburger-nav {
  display: none;
}

.hamburger-nav i{
  font-size: 2em;
}

div.header-button {
  background: var(--clr-secondary);
  margin-left: auto;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 1em;
  height: 100%;
}

div.header-button > * {
  text-shadow: none;
  text-align: center;
  line-height: 22px;
  color: var(--clr-text-secondary);
}

div.header-button a{
  min-width: 60px;
  text-decoration: none;
  font-family: OpenSans,sans-serif,arial;
  font-size: .7em;
  text-transform: uppercase;
  letter-spacing: .075em;
  font-weight: 400;
  padding: 0 2em;
}

div.header-button a span {
  display: inline-block;
  vertical-align: middle;
}
/*#endregion Header */

