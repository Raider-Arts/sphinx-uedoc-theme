  width: 100%;
  height: 100%;
  position: static;
}

.sidebar-list-hover:hover{
  background-color: hsl(var(--clr-primary-hs), 94%);
}

.sidebar-retracted {
  left: -300px !important;
  float: left;
}

div#sidebar p,
div#sidebar a {
  display: block;
  text-overflow: ellipsis;
  overflow: hidden;
}

@media only screen and (max-width: 1280px) {
  div#sidebar {
    position: absolute !important;
    left: 0;
    bottom: 0;
    top: 0;
    z-index: 1010;
    background-color: var(--clr-background);
    width: 300px !important;
    height: 100% !important;
  }

  div.sphinxsidebar {
    position: fixed;
    width: 300px !important;
    overflow: hidden auto;
  }
}


div#sidebar{
  position: sticky;
  top: 0;
  /* overflow: auto; */
  white-space: nowrap;
  transition: left .5s linear 0s;
}

div.sphinxsidebarwrapper {
  padding: 0 !important;
}


div.sphinxsidebar {
  float: none;
  width: 100%;
  margin-left: 0;
  font-size: initial;
  overflow: hidden;
}

div.gutter {
  position: relative;
  cursor: col-resize;
  z-index: 1002;
}

div.gutter::before{
  position: absolute;
  content: "";
  height: 100%;
  width: 0px;
  background-color: hsl(var(--clr-background-hs), 88%);
  transform: translate(-4px);
}


div.gutter {
  width: 3px !important;
}

div.gutter:hover::before{
  width: 11px;
}

div.gutter:active::after {
  transform: translate(-4px);
  width: 11px;
  background-color: hsl(var(--clr-background-hs), 63%);
  background-image: url('data:image/png;base64,');
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 7px 18px;
}

div.gutter::after{
  position: absolute;
  content: "";
  height: 100%;
  width: 3px;
  /* left: 50%; */
  background-color: hsl(var(--clr-background-hs), 75%);
  /* transform: translate(-50%, 0); */
}

div.gutter:active::before{
  display: none;
}


