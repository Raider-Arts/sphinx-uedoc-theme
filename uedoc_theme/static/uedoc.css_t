@import 'basic.css';
@import 'fonts.css';

:root{

  /* This variables can be automatically set from the theme, see Theming docs*/
  --clr-background-h: 0;
  --clr-background-s: 0%;
  --clr-background-l: 100%;

  --clr-primary-h: 0;
  --clr-primary-s: 0%;
  --clr-primary-l: 16%;

  --clr-secondary-h: 197;
  --clr-secondary-s: 92%;
  --clr-secondary-l: 49%;

  --clr-text-primary-h: 0;
  --clr-text-primary-s: 0%;
  --clr-text-primary-l: 100%;

  --clr-text-secondary-h: 0;
  --clr-text-secondary-s: 0%;
  --clr-text-secondary-l: 100%;

  --clr-text-background-h: 0;
  --clr-text-background-s: 0%;
  --clr-text-background-l: 0%;
  
  /* 
    Automatically generated values, please do not override from theme,
    If you still want to override them be sure to respect the original format
  */
  --clr-primary-hs: var(--clr-primary-h),var(--clr-primary-s);
  --clr-background-hs: var(--clr-background-h),var(--clr-background-s);
  --clr-secondary-hs: var(--clr-secondary-h),var(--clr-secondary-s);
  --clr-text-primary-hs: var(--clr-text-primary-h),var(--clr-text-primary-s);
  --clr-text-secondary-hs: var(--clr-text-secondary-h),var(--clr-text-secondary-s);
  --clr-text-background-hs: var(--clr-text-background-h),var(--clr-text-background-s);

  --clr-background-hsl: var(--clr-background-hs), var(--clr-background-l);
  --clr-primary-hsl: var(--clr-primary-hs), var(--clr-primary-l);
  --clr-secondary-hsl: var(--clr-secondary-hs), var(--clr-secondary-l);
  --clr-text-primary-hsl: var(--clr-text-primary-hs), var(--clr-text-primary-l);
  --clr-text-secondary-hsl: var(--clr-text-secondary-hs), var(--clr-text-secondary-l);
  --clr-text-background-hsl: var(--clr-text-background-hs), var(--clr-text-background-l);
  
  /* This variables can be set/overwritten from the theme */
  --clr-background: hsl(var(--clr-background-hsl));
  --clr-primary: hsl(var(--clr-primary-hsl));
  --clr-secondary: hsl(var(--clr-secondary-hsl));

  --clr-primary-gradient-start: hsla(var(--clr-primary-hsl), 0.0);
  --clr-primary-gradient-end: hsla(var(--clr-primary-hsl), 0.95);

  --clr-text-primary: hsl(var(--clr-text-primary-hsl));
  --clr-text-secondary: hsl(var(--clr-text-secondary-hsl));
  --clr-text-background: hsl(var(--clr-text-background-hsl));

}


/*#region Global */

body {
  background-color: var(--clr-background);
  margin: 0;
  font-size: 13px;
  font-family: OpenSans_Regular, Verdana, Arial, sans serif;
  color: var(--clr-text-background);
  line-height: 1.75em;
  padding: 0;
}

div.documentwrapper {
  position: relative;
  display: flex;
  flex-direction: row;
}

h1,
h2,
h3,
h4,
h5,
h6,
.toctree-wrapper p.caption {
  font-size: 1.5em;
  overflow: hidden;
  font-family: Brutal_Regular, Verdana, Arial, sans serif;
  -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
  -moz-box-sizing: border-box;    /* Firefox, other Gecko */
  box-sizing: border-box;         /* Opera/IE 8+ */
}

h2 {
  font-size: 1.35em;
}

h3 {
  font-size: 1.25em;
}

h4,h5,h6 {
  font-size: 1.15em;
}


h1 > a,
h2 > a,
h3 > a,
h4 > a,
h5 > a,
h6 > a,
dt > a{
  color: var(--clr-text-background);
  text-decoration: none;
}

h2,
h3,
h4,
h5,
h6 {
  margin-top: 1.75em;
  line-height: 1em;
  margin-bottom: 1.25em;
}

h2,
h3,
h4,
h5,
h6 {
  padding: 4px 6px 5px;
  /* font-size: 1.5em; */
  font-weight: bold;
  clear: both;
  margin-left: -6px;
  margin-right: -6px;
}

h1 > a:visited,
h2 > a:visited,
h3 > a:visited,
h4 > a:visited,
h5 > a:visited,
h6 > a:visited,
dt > a:visited {
  color: var(--clr-text-background-dark);
  text-decoration: none;
}

div.body {
  width: 100%;
  min-width: 0;
  max-width: none;
}

p {
  font-size: 1em;
  margin-top: 8px;
  margin-bottom: 20px;
}

a:visited {
  background-color: transparent;
  text-decoration: underline;
}

a {
  color: hsl(var(--clr-text-background-hs, 19%));
  /* #303030; */
}

html {
  --scrollbarBG: transparent;
  --thumbBG: hsla(var(--clr-primary-hs), 82%, 0.6);
}

*::-webkit-scrollbar {
  width: 8px;
}

* {
  scrollbar-width: thin;
  scrollbar-color: var(--thumbBG) var(--scrollbarBG);
}

*::-webkit-scrollbar-track {
  background: var(--scrollbarBG);
}

*::-webkit-scrollbar-thumb {
  background-color: var(--thumbBG);
  border-radius: 6px;
}

.full-width {
  width: 100%;
}

/*#endregion Global */

/*#region Document */

div.body{
  background-color: transparent;
  padding: 47px 72px;
  margin: 0;
  border: 0 solid hsl(var(--clr-background-hs), 94%);
  /* hsl(0, 0%, 94%); */
  width: calc(100% - 440px);
}

.bodywrapper {
  width: 100%;
}

.section {
  padding-bottom: 1em;
}

div.section .section {
  padding-bottom: unset;
}

@media only screen and (max-width: 1280px) {

  .bodywrapper div.body div.section:first-of-type h1 {
    margin: -10px 0 0 -47px;
  }

  div.body {
    padding: 10px 30px 30px;
    width: auto;
  }
}

@media only screen and (max-width: 1023px){
  div.body {
    padding: 10px 30px 30px;
    width: auto;
  }
}

div.body div.section:first-of-type h1,
div.topic {
  width: 100%;
  background-color: transparent;
  background-image: -moz-linear-gradient(
    right,
    var(--clr-primary-gradient-start) 35%,
    var(--clr-primary-gradient-end) 85%
  );
  background-image: linear-gradient(
    to left,
    var(--clr-primary-gradient-start) 35%,
    var(--clr-primary-gradient-end) 85%
  );
  background-repeat: no-repeat;
  background-size: cover;
  position: relative;
  display: inline-block;
  font-size: 2em;
  padding-top: 2em;
  padding-bottom: 2em;
  padding-left: 72px;
  color: var(--clr-text-primary);
  margin: -47px 0 0 -72px;
  text-shadow: 0 2px 10px hsla(var(--clr-primary-hs), 0%, 0.75);
}


@media only screen and (max-width: 1280px){
  .bodywrapper div.body div.section:first-of-type h1 {
      font-size: 1.675em;
      padding: 2em 47px;
  }
}

@media only screen and (max-width: 700px){
  .bodywrapper div.body div.section:first-of-type h1 {
      font-size: 1.675em;
      padding: 2em 47px !important;
  }
}

div.body div.section:first-of-type h1 a{
  color: var(--clr-text-primary);
}


.toctree-wrapper p.caption {
  font-weight: 600;
}

.section div.contents {
  background-image: none;
  filter: none;
  font-size: inherit;
  color: inherit;
  text-shadow: none;
  margin-left: 0;
  padding-left: 0;
}

div.topic {
  border: none;
  margin: 0;
  margin-left: -72px;
}

div.topic:not(.contents) p:not(.topic-title) {
  font-weight: normal;
  font-size: 0.55em;
  line-height: 1.375em;
  margin: 19px 0 0;
  color: var(--clr-text-primary);
  text-shadow: 0 2px 10px hsla(var(--clr-primary-hs), 0%, 0.75);
}

div.admonition {
  padding: 27px 25px 27px 25px;
  margin: .5em 1em;
  border-radius: 0;
  overflow: hidden;
  position: relative;
}

.note, 
.hint {
  border: solid 1px #e8d192;
  background-color: rgba(245,240,217,.75);
}

.tip,
.seealso {
  border:1px solid #acd2ac;
  background-color:rgba(192,255,192,.25);
}

.warning,
.attention,
.error,
.danger,
.important {
  border:1px solid #ecb7b7;
  background-color:rgba(255,192,192,.375);
}

.note .admonition-title, 
.hint .admonition-title, 
.warning .admonition-title,
.tip .admonition-title,
.seealso .admonition-title,
.attention .admonition-title,
.danger .admonition-title,
.error .admonition-title,
.important .admonition-title{
  display: block;
  content: "";
  position: absolute;
  top: 5px;
  left: 10px;
  font-family: Brutal_Regular;
  font-weight: bold;
  letter-spacing: 3px;
  opacity: .75;
  line-height: 1.5em;
  text-transform: uppercase;
}

/** Tables */

table {
  background-color: transparent;
  border-width: 0 !important;
  border-spacing: 0;
  width: 100%;
  margin: 1.5em 0 !important;
  font-size: .925em;
}

th {
  background-color: hsl(var(--clr-background-hs), 85%);
  color: var(--clr-text-background);
  padding: 13px 22px !important;
  vertical-align: top;
  text-align: left;
  font-weight: bold;
  line-height: 2.14;
  font-size: 1.125em;
}

td {  
  border-top: 1px solid hsl(var(--clr-background-hs), 75%) !important;
  background-color: transparent;
  vertical-align: top;
  padding: 13px 22px !important;
  height: 47px;
}

@media only screen and (max-width: 450px) {
  table {
    table-layout: fixed;
  }
}

/** lists **/

ul, ol, dl {
  margin: .75em 0 1.125em 2em;
  padding: 0 0;
  padding-left: 10px;
}

ol {
  list-style: decimal;
}

ul {
  list-style: square outside;
}

li {
  line-height: 2.5em;
  padding: 0 0;
}

ol ol ol,ol ul ol,ul ul ol,ul ol ol{
  list-style:lower-roman
}

ol li {
  padding: .375em 0;
}


/** Code **/
div.highlight {
  padding-left: 0.75em;
}

table.highlighttable td {
  height: auto;
  padding: 0 !important;
}

table.highlighttable div.linenodiv {
  padding-left: 5px;
}

/** flexbox utilities **/
.break-column {
  flex-basis: 100%;
  width: 0;
}

.break {
  flex-basis: 100%;
  height: 0;
}


/*#endregion Global */

/*#region Headerlink */
:not(h1) a.headerlink {
  font-size: 12px;
}

a.headerlink {
  text-decoration: none;
  font-size: 14px;
}

h1:hover > a.headerlink,
h2:hover > a.headerlink,
h3:hover > a.headerlink,
h4:hover > a.headerlink,
h5:hover > a.headerlink,
h6:hover > a.headerlink,
dt:hover > a.headerlink,
caption:hover > a.headerlink,
p.caption:hover > a.headerlink,
div.code-block-caption:hover > a.headerlink {
  visibility: hidden;
}

h1:hover > a.headerlink::after,
h2:hover > a.headerlink::after,
h3:hover > a.headerlink::after,
h4:hover > a.headerlink::after,
h5:hover > a.headerlink::after,
h6:hover > a.headerlink::after,
dt:hover > a.headerlink::after,
caption:hover > a.headerlink::after,
p.caption:hover > a.headerlink::after,
div.code-block-caption:hover > a.headerlink {
  font-family: "Font Awesome 5 Free";
  content: "\f0c1";
  visibility: visible;
  font-weight: 600;
}
/*#endregion Headerlink */

/*#region Header */

.nav-menu-header-mobile {
  width: 90.75vw;
}

.scroll-block {
  height: 100%;
  overflow: hidden;
}

div#nav-menu {
  transition: background-color 0.2s ease-in;
}

div#nav-menu div.nav-menu-wrapper {
  transition: right 0.1s ease-in;
}

@media only screen and (max-width: 767px) {
  .uedoc-header div#nav-menu{
    position: absolute;
    display: flex;
    top: 54px;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 1012;
    justify-content: flex-end;
  }

  .open-nav{
    background-color: rgba(0, 0, 0, 0.75);
  }

  div.open-nav div.nav-menu-wrapper{
    right: 0 !important;
  }


