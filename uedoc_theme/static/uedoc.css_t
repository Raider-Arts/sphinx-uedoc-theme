    height: calc(100% - 54px);
    width: 90.75vw;
    max-width: 100%;
    flex-direction: column;
    align-items: stretch;
    position: relative;
    right: -90.75vw;
  }

  div.uedoc-quicklinks-wrapper {
    height: unset;
  }

  div#nav-menu div.header-button {
    height: unset;
    margin-top: auto;
