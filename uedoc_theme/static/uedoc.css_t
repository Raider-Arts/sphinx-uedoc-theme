@import 'basic.css';
@import 'fonts.css';


/** 
  Debug Macro: jQuery(window).keydown(function(e) { if (e.keyCode == 123) debugger; });
  Stops the page with the F12 key  
**/

/*#region Global */

body {
  background-color: #ffffff;
  margin: 0;
  font-size: 13px;
  font-family: OpenSans_Regular, Verdana, Arial, sans serif;
  color: #000;
  line-height: 1.75em;
  margin: 0;
  padding: 0;
}

div.documentwrapper {
  position: relative;
  display: flex;
  flex-direction: row;
}

@media only screen and (max-width: 700px){
  h1 {
    padding: 0 0 0 30px !important;;
  }
}

h1 {
  padding: 0 0 0 72px;
  font-size: 1.5em;
  overflow: hidden;
  font-family: Brutal_Regular, Verdana, Arial, sans serif;
  height: 100%;
  -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
  -moz-box-sizing: border-box;    /* Firefox, other Gecko */
  box-sizing: border-box;         /* Opera/IE 8+ */
}

h1 > a,
h2 > a,
h3 > a,
h4 > a,
h5 > a,
h6 > a,
dt > a{
  color: #000;
  text-decoration: none;
}

h1 > a:visited,
h2 > a:visited,
h3 > a:visited,
h4 > a:visited,
h5 > a:visited,
h6 > a:visited,
dt > a:visited {
  color: #000;
  text-decoration: none;
}

div.body {
  width: 100%;
  min-width: 0;
  max-width: none;
}

p {
  font-size: 1em;
  margin-top: 8px;
  margin-bottom: 20px;
}
a:visited {
  background-color: transparent;
  text-decoration: underline;
}

a {
  color: #303030;
}

div.body div.section:first-of-type h1 {
  width: 100%;
  background-color: transparent;
  background-image: linear-gradient(
    to left,
    rgba(42, 42, 42, 0) 35%,
    rgba(42, 42, 42, 0.95) 85%
  );
  background-image: -moz-linear-gradient(
    right,
    rgba(42, 42, 42, 0) 35%,
    rgba(42, 42, 42, 0.95) 85%
  );
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fD2a2a2a',endColorstr='#002a2a2a',GradientType=1);
  background-repeat: no-repeat;
  background-size: cover;
  position: relative;
  display: inline-block;
  font-size: 2em;
  margin: 0;
  padding: 2em 2em;
  color: #fff;
  text-shadow: 0 2px 10px rgba(0, 0, 0, 0.75);
}

@media only screen and (max-width: 700px){
  div.body div.section:first-of-type h1 {
      font-size: 1.675em;
      padding: 2em 30px !important;;
  }
}

div.body div.section:first-of-type h1 a{
  color: #fff;
}

div.contents {
  background-image: none !important;
  filter: none !important;
  font-size: inherit !important;
  color: inherit !important;
  text-shadow: none !important;
}

div.topic {
  border: none;
  /* width: 100%; */
  background-color: transparent;
  background-image: linear-gradient(
    to left,
    rgba(42, 42, 42, 0) 35%,
    rgba(42, 42, 42, 0.95) 85%
  );
  background-image: -moz-linear-gradient(
    right,
    rgba(42, 42, 42, 0) 35%,
    rgba(42, 42, 42, 0.95) 85%
  );
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fD2a2a2a',endColorstr='#002a2a2a',GradientType=1);
  background-repeat: no-repeat;
  background-size: cover;
  position: relative;
  display: inline-block;
  font-size: 2em;
  margin: 0;
  padding: 2em 2em;
  color: #fff;
  text-shadow: 0 2px 10px rgba(0, 0, 0, 0.75);
}

/*#endregion Global */

/*#region Headerlink */

a.headerlink {
  text-decoration: none;
  font-size: 14px;
}

h1:hover > a.headerlink,
h2:hover > a.headerlink,
h3:hover > a.headerlink,
h4:hover > a.headerlink,
h5:hover > a.headerlink,
h6:hover > a.headerlink,
dt:hover > a.headerlink,
caption:hover > a.headerlink,
p.caption:hover > a.headerlink,
div.code-block-caption:hover > a.headerlink {
  visibility: hidden;
}

h1:hover > a.headerlink::after,
h2:hover > a.headerlink::after,
h3:hover > a.headerlink::after,
h4:hover > a.headerlink::after,
h5:hover > a.headerlink::after,
h6:hover > a.headerlink::after,
dt:hover > a.headerlink::after,
caption:hover > a.headerlink::after,
p.caption:hover > a.headerlink::after,
div.code-block-caption:hover > a.headerlink {
  font-family: "Font Awesome 5 Free";
  content: "\f0c1";
  visibility: visible;
}
/*#endregion Headerlink */

/*#region Header */

.uedoc-header {
  width: 100%;
  clear: both;
  height: 54px;
  background-color: #2a2a2a;
  display: flex;
  align-items: center;
}

@media only screen and (max-width: 1280px) {
  div.uedoc-title {
    font-size: 1.3em;
  }
}

@media only screen and (max-width: 767px) {
  div.uedoc-title {
    font-size: 1.1em;
  }
}

@media only screen and (max-width: 450px) {
  div.uedoc-title {
    font-size: 1em;
  }
}

.uedoc-title {
  color: white;
  font-family: Verdana;
  font-weight: 700;
  font-size: 1.5em;
  margin: 0 40px 0 20px;
}

.uedoc-logo {
  width: 3.7em;
  height: 3.7em;
  margin-left: .5em;
  background: url('{{theme_logo}}') 50% no-repeat;
  background-size: 50px auto;
}

.uedoc-quicklinks {
  list-style-type: none;
  height: 100%;
  padding: 0;
  margin: 0;
}

.uedoc-quicklinks li {
  position: relative;
  height: 100%;
}

.uedoc-quicklinks-wrapper {
  height: 100%;
}

.uedoc-quicklinks li::before {
  background: #0aaff1;
  content: "";
  display: block;
  position: absolute;
  bottom: 3px;
  left: 0;
  width: 100%;
  height: 0;
  -webkit-transition: height .2s ease-in-out;
  -o-transition: height .2s ease-in-out;
  transition: height .2s ease-in-out;
}

.uedoc-quicklinks li:hover::before {
  height: 5px;
}

.uedoc-quicklinks p {
  color: #ccc;
  margin: 0;
  font-size: .85em;
}


.uedoc-quicklinks a {
  display: block;
  font-family: OpenSans,sans-serif,arial;
  text-transform: uppercase;
  letter-spacing: .075em;
  padding: 0 1em;
  height: 100%;
  text-decoration: none;
  display: flex;
  align-items: center;
}

.uedoc-quicklinks a:visited {
  text-decoration: none;
}

/*#endregion Header */

/*#region Sidebar */

.sidebar-retracted {
  left: -300px !important;
  float: left;
}


@media only screen and (max-width: 1280px) {
  div#sidebar {
    position: absolute !important;
    left: 0;
    bottom: 0;
    top: 0;
    z-index: 1010;
    background-color: white;
    width: 300px !important;
    height: 100% !important;
  }

  div.sphinxsidebar {
    position: fixed;
    width: 300px !important;
    overflow: auto;
  }
}


div#sidebar{
  position: sticky;
  top: 0;
  overflow: auto;
  transition: left .5s linear 0s;
}

div.sphinxsidebarwrapper {
  padding: 0 !important;
}


div.sphinxsidebar {
  float: none;
  width: 100%;
  margin-left: 0;
  font-size: initial;
}

div.gutter {
  position: relative;
  cursor: col-resize;
  z-index: 1002;
}

div.gutter::before{
  position: absolute;
  content: "";
  height: 100%;
  width: 0px;
  background-color: #e0e0e0;
  transform: translate(-4px);
}


div.gutter {
  width: 3px !important;
}

div.gutter:hover::before{
  width: 11px;
}

div.gutter:active::after {
  transform: translate(-4px);
  width: 11px;
  background-color: #a0a0a0;
  background-image: url('data:image/png;base64,');
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 7px 18px;
}

div.gutter::after{
  position: absolute;
  content: "";
  height: 100%;
  width: 3px;
  /* left: 50%; */
  background-color: #c0c0c0;
  /* transform: translate(-50%, 0); */
}

div.gutter:active::before{
  display: none;
}


/*#region Navigation */

@media only screen and (max-width: 1280px) {
  div#navigation {
      line-height: 1em;
      font-size: 1.5em;
      top: 145px;
  }
}

#navigation span {
  opacity: .5;
}

#navigation {
  transform: translateY(0) rotate(0deg);
  transition: left .5s linear 0s;
  transform-origin: left center 0;
  float: left;
  margin: 0;
  padding: 3px 8px;
  position: fixed;
  left: 300px;
  top: 152px;
  z-index: 1001;
  background-color: #d8d8d8;
  color: #000;
  -webkit-box-shadow: 4px -3px 6px 0 rgba(50,50,50,.75);
  -moz-box-shadow: 4px -3px 6px 0 rgba(50,50,50,.75);
  box-shadow: 0 2px 4px 0 rgba(0,0,0,0);
  cursor: pointer;
  border: 1px solid #a0a0a0;
  font-size: 2.5em;
}

/*#endregion Navigation */

/*#region Filter Pages */

@media only screen and (max-width: 1280px) {
  div#searchbox{
    padding-right: 42px;
  }
}

form.search {
  padding: 1px 0 1px 1px;
}

div.sphinxsidebar form {
  margin-top: 0 !important;
}

#searchbox input {
  position: relative;
  height: 32px;
  width: calc(100% - 32px) !important;
  padding: 4px 16px !important;
  border-width: 0 0 1px 0;
  border-color: #767676;
  box-sizing: initial !important;
  font-family: Arial;
  font-size: 0.85em;
}

/*#endregion Filter Pages  */

/*#endregion Sidebar */


/*#region Related */

div.related {
  height: 83px;
  padding: 0;
  border-bottom: 1px solid #e0e0e0;
  overflow: hidden;
  display: flex;
  align-items: center;
}

/*#endregion Related */


